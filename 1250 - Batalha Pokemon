#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    double v1, v2, d1, d2, k, i, tur1, n;
    scanf("%lf", &n);

    for(i=1; i<=n; i++){
        scanf("%lf %lf %lf %lf", &v1, &v2, &d1, &d2);

            while(k==0){
                tur1=ceil(v1/d2);

                    if((tur1*d1)>=v2){
                        printf("Clodes\n");
                            k=1;
                    }
                    else{
                        if(d2>=v1){
                            printf("Bezaliel\n");
                            k=1;
                        }
                    else{
                        v1=v1-d2; d1=d1+50;
                    }
                    }
            }k=0;
    }
	return 0;
}
