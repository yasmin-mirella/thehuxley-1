#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    double a, b, c;
    scanf("%lf %lf %lf", &a, &b, &c);

    if(a+b>c&&a+c>b&&c+b>a){
        if(a!=b&& b!=c&&a!=c){
            printf("ESCALENO");
        }
       
        else if(a==b&&b==c){
            printf("EQUILATERO");
        }
         else{
            printf("ISOSCELES");
        }
        
    }
    else{
        printf("-");
    }
    
    
	return 0;
}
